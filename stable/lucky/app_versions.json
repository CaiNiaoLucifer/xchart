                        "editable": false,
                        "default": "/goodluck"
                      }
                    },
                    {
                      "variable": "hostPathEnabled",
                      "label": "Enable Host Path for Lucky Data Volume",
                      "schema": {
                        "type": "boolean",
                        "default": false,
                        "show_subquestions_if": true,
                        "subquestions": [
                          {
                            "variable": "hostPath",
                            "label": "Host Path for Lucky Data Volume",
                            "schema": {
                              "type": "hostpath",
                              "required": true
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
