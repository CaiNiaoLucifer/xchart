          }
        },
        {
          "variable": "hostNetwork",
          "label": "Configure Host Network",
          "group": "Networking",
          "schema": {
            "type": "boolean",
            "default": false
          }
        },
        {
          "variable": "ingressEnabled",
          "group": "Ingress",
          "label": "Enable Ingress",
          "description": "config ingress, you must install traefik first",
          "schema": {
            "type": "boolean",
            "default": false,
            "show_subquestions_if": true,
            "subquestions": [
              {
                "variable": "certificate",
                "label": "Certificate Configuration",
                "schema": {
                  "type": "int",
                  "$ref": [
                    "definitions/certificate"
                  ]
                }
              },
              {
                "variable": "host",
                "label": "HostName",
                "schema": {
                  "type": "string",
                  "default": "test.test.org",
                  "required": true
                }
              },
              {
                "variable": "path",
                "label": "Path",
                "schema": {
                  "type": "string",
                  "required": true,
                  "default": "/"
                }
              },
              {
                "variable": "pathType",
                "label": "Path Type",
                "schema": {
                  "type": "string",
                  "required": true,
                  "default": "Prefix"
                }
              }
            ]
          }
        },
        {
          "variable": "dnsConfig",
          "label": "DNS Configuration",
          "group": "Advanced DNS Settings",
          "schema": {
            "type": "dict",
            "attrs": [
              {
                "variable": "options",
                "label": "DNS Options",
                "schema": {
                  "type": "list",
                  "items": [
                    {
                      "variable": "optionsEntry",
                      "label": "Option Entry Configuration",
                      "schema": {
                        "type": "dict",
                        "attrs": [
                          {
                            "variable": "name",
                            "label": "Option Name",
                            "schema": {
                              "type": "string",
                              "required": true
                            }
                          },
                          {
                            "variable": "value",
                            "label": "Option Value",
                            "schema": {
                              "type": "string",
                              "required": true
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "variable": "ownerUID",
          "label": "Owner User ID",
          "group": "Configuration",
          "schema": {
            "type": "int",
            "default": 1000,
            "min": 1,
            "max": 65535
          }
        },
        {
          "variable": "ownerGID",
          "label": "Owner Group ID",
          "group": "Configuration",
          "schema": {
            "type": "int",
            "default": 1000,
            "min": 1,
            "max": 65535
          }
        },
        {
          "variable": "environmentVariables",
          "label": "alist environment",
          "group": "Configuration",
          "schema": {
            "type": "list",
            "default": [],
            "items": [
              {
                "variable": "environmentVariable",
                "label": "Environment Variable",
                "schema": {
                  "type": "dict",
                  "attrs": [
                    {
                      "variable": "name",
                      "label": "Name",
                      "schema": {
                        "type": "string"
                      }
                    },
                    {
                      "variable": "value",
                      "label": "Value",
                      "schema": {
                        "type": "string"
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "variable": "appVolumeMounts",
          "label": "alist Storage",
          "group": "Storage",
          "schema": {
            "type": "dict",
            "attrs": [
              {
                "variable": "config",
                "label": "alist config",
                "schema": {
                  "type": "dict",
                  "attrs": [
                    {
                      "variable": "datasetName",
                      "label": "alist config Dataset Name",
                      "schema": {
                        "type": "string",
                        "hidden": true,
                        "$ref": [
                          "normalize/ixVolume"
                        ],
                        "show_if": [
                          [
                            "hostPathEnabled",
                            "=",
                            false
                          ]
                        ],
                        "default": "ix-alist_config",
                        "editable": false
                      }
                    },
                    {
                      "variable": "mountPath",
                      "label": "alist home Mount Path",
                      "description": "Path where the volume will be mounted inside the pod",
                      "schema": {
                        "type": "path",
                        "hidden": true,
                        "editable": true,
                        "default": "/opt/alist/data"
                      }
                    },
                    {
                      "variable": "hostPathEnabled",
                      "label": "Enable Custom Host Path for alist config",
                      "schema": {
                        "type": "boolean",
                        "default": false,
                        "show_subquestions_if": true,
                        "subquestions": [
                          {
                            "variable": "hostPath",
                            "label": "Host Path for alist home",
                            "schema": {
                              "type": "hostpath",
                              "required": true
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "variable": "extraAppVolumeMounts",
          "label": "Extra Host Path Volumes",
          "group": "Storage",
          "schema": {
            "type": "list",
            "items": [
              {
                "variable": "extraAppVolume",
                "label": "Host Path Volume",
                "description": "Add an extra host path volume for alist application",
                "schema": {
                  "type": "dict",
                  "attrs": [
                    {
                      "variable": "mountPath",
                      "label": "Mount Path in Pod",
                      "description": "Path where the volume will be mounted inside the pod",
                      "schema": {
                        "type": "path",
                        "required": true
                      }
                    },
                    {
                      "variable": "hostPath",
                      "label": "Host Path",
                      "description": "Host path",
                      "schema": {
                        "type": "hostpath",
                        "required": true
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "variable": "enableResourceLimits",
          "label": "Enable Pod resource limits",
          "group": "Resource Limits",
          "schema": {
            "type": "boolean",
            "default": false
          }
        },
        {
          "variable": "cpuLimit",
          "label": "CPU Limresource limitsit",
          "description": "CPU resource limit allow  plain integer values with suffix m(milli) e.g 1000m, 100.",
          "group": "Resource Limits",
          "schema": {
            "type": "string",
            "show_if": [
              [
                "enableResourceLimits",
                "=",
                true
              ]
            ],
            "valid_chars": "^\\d+(?:\\.\\d+(?!.*m$)|m?$)",
            "default": "4000m"
          }
        },
        {
          "variable": "memLimit",
          "label": "Memory Limit",
          "group": "Resource Limits",
          "description": "Memory limits is specified by number of bytes. Followed by quantity suffix like E,P,T,G,M,k and Ei,Pi,Ti,Mi,Gi,Ki can also be used. e.g 129e6, 129M,  128974848000m, 123Mi",
          "schema": {
            "type": "string",
            "show_if": [
              [
                "enableResourceLimits",
                "=",
                true
              ]
            ],
            "valid_chars": "^([+-]?[0-9.]+)([eEinumkKMGTP]*[-+]?[0-9]*)$",
            "default": "8Gi"
          }
        }
      ]
    },
